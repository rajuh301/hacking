generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model LabSubmission {
  id        String   @id @default(cuid())
  email     String
  password  String
  userAgent String?  // To see what device/browser the "victim" used
  ipAddress String?  // For geographical analysis in a lab setting
  createdAt DateTime @default(now())
}